    link(href='https://db.onlinewebfonts.com/c/604c3826d04854ecb7e53030f4acda4c?family=BuenosAires-Regular' rel='stylesheet')
    link(rel='stylesheet' href='https://unpkg.com/swiper/swiper-bundle.min.css')
    script(src='https://unpkg.com/swiper/swiper-bundle.min.js')
    link(rel="stylesheet" href="/output.css")
    link(rel="stylesheet" href="/index.css")

  body(class="text-white")

    header(class="flex justify-between w-full lg:px-[80px] px-4 sticky top-0 bg-violet lg:py-8 z-50 py-6")
      picture 
        img(src="/assets/svgs/logo.svg" width="auto" height="auto" class="logo")
      nav(class="hidden lg:flex lg:justify-center lg:items-center lg:gap-8")
        each item in services
          span 
            a.scroll-link(href=item.url) #{item.name}
        button(class="border border-white rounded-lg px-6 py-3 secondary") Course Finder

    <!-- hero section -->
    section(class="hero flex flex-col")

      <!-- hero body -->
      div(class="flex justify-center lg:py-20 flex-col items-center lg:px-[240px] px-4 relative gap-4")

