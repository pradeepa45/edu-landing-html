                div.py-4.flex.flex-col(class="px-[30px] max-w-[300px]")
                  p.text-blue.text-sm.leading-8
                    a(href='') #{event.time}
                  p.text-dark.capitalize(class="text-[22px]" title=event.name) #{event.name}
                  p.text-grey.line-clamp-2.text-ellipsis #{event.description}
                  button.mt-5.flex.gap-3.items-center.text-primary.font-medium.capitalize.mb-12
                    span Register now
                    svg(width="9" height="12" viewBox="0 0 9 12" fill="none" xmlns="http://www.w3.org/2000/svg")
                      path(d="M1.71387 1L7.42815 5.7619L1.71387 11" stroke="#FF7361" stroke-width="2" stroke-linecap="round" stroke-linejoin="round")


      button.flex.gap-3.items-center.text-primary.font-medium.capitalize
        span See more
